{
  "extends": ["next", "airbnb-typescript", "plugin:react-perf/recommended", "prettier"],
  "parserOptions": {
    "project": ["./tsconfig.json"]
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "react/display-name": 0,
    "@typescript-eslint/consistent-type-imports": "error"
  }
}